<h1>Client List</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Pincode</th>
      <th>Date of birth</th>
      <th>Mobile</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Education</th>
      <th>Occupation</th>
      <th>Job post</th>
      <th>Name of company</th>
      <th>Job expirience year</th>
      <th>Income</th>
      <th>Economical liability</th>
      <th>Number of child</th>
      <th>Child 1 Age</th>
      <th>Child 1 Date of Birth</th>
      <th>Child 2 Age</th>
      <th>Child 2 Date of Birth</th>
      <th>Child 3 Age</th>
      <th>Child 3 Date of Birth</th>
      <th>Child 4 Age</th>
      <th>Child 4 Date of Birth</th>
      <th>Anniversary date</th>
      <th>Short Term Goal</th>
      <th>Long Term Goal</th>
      <th>Retirement age</th>
      <th>Child Education</th>
      <th>Child Marriage</th>
      <th>Retirement Pension</th>
      <th>Fixed Income</th>
      <th>Equity</th>
      <th>Gold</th>
      <th>Land and Estate</th>
      <th>House Owned</th>
      <th>House Rented</th>
      <th>House Co Provided</th>
      <th>Four Wheeler</th>
      <th>Two Wheeler</th>
      <th>No Vehicle</th>
      <th></th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @client_infos.each do |client_info| %>
      <tr>
        <td><%= client_info.name %></td>
        <td><%= client_info.address %></td>
        <td><%= client_info.pincode %></td>
        <td><%= client_info.date_of_birth %></td>
        <td><%= client_info.mobile %></td>
        <td><%= client_info.phone %></td>
        <td><%= client_info.email %></td>
        <td><%= client_info.education %></td>
        <td><%= client_info.occupation %></td>
        <td><%= client_info.job_post %></td>
        <td><%= client_info.name_of_company %></td>
        <td><%= client_info.job_expirience_year %></td>
        <td><%= client_info.income %></td>
        <td><%= client_info.economical_liability %></td>
        <td><%= client_info.number_of_child %></td>
        <% client_info.child_infos.each do |child| %>
            <td><%= child.age %></td>
            <td><%= child.date_of_birth %></td>
        <% end %>
        <td><%= client_info.anniversary_date %></td>
        <td><%= client_info.short_term_goal %></td>
        <td><%= client_info.long_term_goal %></td>
        <td><%= client_info.retirement_age %></td>
        <td><%= client_info.plan_child_education %></td>
        <td><%= client_info.plan_child_marriage %></td>
        <td><%= client_info.plan_retirement_fund %></td>
        <td><%= client_info.investment_type.fix_income %></td>
        <td><%= client_info.investment_type.equity %></td>
        <td><%= client_info.investment_type.gold %></td>
        <td><%= client_info.investment_type.land_and_estate %></td>
        <td><%= client_info.house.owned %></td>
        <td><%= client_info.house.rented %></td>
        <td><%= client_info.house.co_provider %></td>
        <td><%= client_info.vehicle.four_wheeler %></td>
        <td><%= client_info.vehicle.two_wheeler %></td>
        <td><%= client_info.vehicle.none %></td>
        <td><%= link_to 'Show', client_info %></td>
        <td><%= link_to 'Edit', edit_client_info_path(client_info) %></td>
        <td><%= link_to 'Destroy', client_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to "Download Excel", download_xlsx_path(id: client_info.id,format: "xlsx") %> </td>
        <td><%= link_to "Download Pdf", download_pdf_path(id: client_info.id,format: "pdf") %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>

<%= link_to 'New Client info', new_client_info_path %>
<%= link_to "Download Excel", download_xlsx_path(format: "xlsx") %>
<%= link_to "Download Pdf", download_pdf_path(format: "pdf") %>
